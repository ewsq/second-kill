<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.eric.lottery.mapper.test.UserMapper">
    <select id="listUserByUserName" resultType="com.eric.seckill.common.bean.User">
        SELECT
            *
        FROM
            USER
        WHERE
            user_name = #{userName}
    </select>
</mapper>